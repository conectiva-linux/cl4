      selname.options[i].selected = 0;\n\
      gone.value = gone.value + "," + selname.options[i].text;\n\
      for (j=i ; j<selname.options.length ; j++) {\n\
%-cont-%

!*strings.22058:        selname.options[j] = selname.options[j+1];\n\
      }\n\
    }\n\
  }\n\
}\n\
